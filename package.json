{
  "name": "openapi-generator",
  "version": "1.0.0",
  "description": "OpenAPI generator for typescript",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "scripts": {
    "clean": "rm -rf dist/ rm -rf src/",
    "generate": "openapi-generator-cli generate -i $YAML_FILE_PATH -g typescript-fetch -o src --additional-properties=typescriptThreePlus=true",
    "build": "rm -rf src/*.spec.* && tsc"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "typescript": "^4.5.2"
  }
}
